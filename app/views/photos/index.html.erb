<div class="patterned-block">
</div>

<section class="container container-thin centered">
  <h1 class="main-row-reversed sub-row"><%= @pet.name %></h1>
  <div class="photos-composer main-row">
    <% @photos.each_slice(10).reverse_each do |photos_block| %>
      <div class="clearfix photo-block-<%= photos_block.count %>">
        <%= composer_block(photos_block) %>
      </div>
    <% end %>
  </div>
</section>
